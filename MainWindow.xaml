<Window x:Class="FakeAxioma3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FakeAxioma3"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1280" Loaded="window_load">
    <Window.Resources>
        <Style x:Key="FocusVisual">
            <Setter Property="Control.Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Rectangle Margin="2" StrokeDashArray="1 2" Stroke="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" SnapsToDevicePixels="true" StrokeThickness="1"/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <SolidColorBrush x:Key="Button.Static.Background" Color="#FFDDDDDD"/>
        <SolidColorBrush x:Key="Button.Static.Border" Color="#FF707070"/>
        <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#FFBEE6FD"/>
        <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#FF3C7FB1"/>
        <SolidColorBrush x:Key="Button.Pressed.Background" Color="#FFC4E5F6"/>
        <SolidColorBrush x:Key="Button.Pressed.Border" Color="#FF2C628B"/>
        <SolidColorBrush x:Key="Button.Disabled.Background" Color="#FFF4F4F4"/>
        <SolidColorBrush x:Key="Button.Disabled.Border" Color="#FFADB2B5"/>
        <SolidColorBrush x:Key="Button.Disabled.Foreground" Color="#FF838383"/>
        <Style x:Key="compoment_button" TargetType="{x:Type Button}">
            <Setter Property="FocusVisualStyle" Value="{StaticResource FocusVisual}"/>
            <Setter Property="Background" Value="{StaticResource Button.Static.Background}"/>
            <Setter Property="BorderBrush" Value="{StaticResource Button.Static.Border}"/>
            <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Padding" Value="1"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border x:Name="border" CornerRadius="6" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" SnapsToDevicePixels="true">

                            <ContentPresenter x:Name="contentPresenter" Focusable="False" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsDefaulted" Value="true">
                                <Setter Property="BorderBrush" TargetName="border" Value="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
                            </Trigger>
                            <Trigger Property="IsMouseOver" Value="true">
                                <Setter Property="Background" TargetName="border" Value="{StaticResource Button.MouseOver.Background}"/>
                                <Setter Property="BorderBrush" TargetName="border" Value="{StaticResource Button.MouseOver.Border}"/>
                            </Trigger>
                            <Trigger Property="IsPressed" Value="true">
                                <Setter Property="Background" TargetName="border" Value="{StaticResource Button.Pressed.Background}"/>
                                <Setter Property="BorderBrush" TargetName="border" Value="{StaticResource Button.Pressed.Border}"/>
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="false">
                                <Setter Property="Background" TargetName="border" Value="{StaticResource Button.Disabled.Background}"/>
                                <Setter Property="BorderBrush" TargetName="border" Value="{StaticResource Button.Disabled.Border}"/>
                                <Setter Property="TextElement.Foreground" TargetName="contentPresenter" Value="{StaticResource Button.Disabled.Foreground}"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>
    <!--начало. Столбцы строки -->
    <Grid x:Name="base" Background="Pink">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1.15*"/>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.10*"/>
            <RowDefinition Height="0.60*"/>
            <RowDefinition Height="0.30*"/>

        </Grid.RowDefinitions>
        <!--Левая часть.цвет фона -->
        <Rectangle Grid.Column="0"  Grid.RowSpan="3" Fill="white" Opacity="0.5" />
        <!--Название -->
        <Label x:Name="Headerr" Grid.Column="1" FontWeight="ExtraBold" Content="5-координатный станок с ЧПУ IMU-5XP 2518" FontSize ="28" HorizontalAlignment="Left" Margin="219,10,0,0" VerticalAlignment="Top" Width="1242" Grid.ColumnSpan="2"/>

        <!-- Все кнопки в центре-->
        <Button Style="{DynamicResource compoment_button}" Background="LavenderBlush"  Grid.Column="1" Content="Ось Х"  HorizontalAlignment="Left" Margin="20,286,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="69" FontWeight="Bold"/>
        <Button Background="LavenderBlush" Grid.Column="1" Content="Ось А"  HorizontalAlignment="Left" Margin="103,286,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="69" FontWeight="Bold" Style="{DynamicResource compoment_button}"/>
        <Button Background="LavenderBlush" Grid.Column="1" Content="Пульт"  HorizontalAlignment="Left" Margin="185,286,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="69" FontWeight="Bold" Style="{DynamicResource compoment_button}"/>
        <Button Background="LavenderBlush" Grid.Column="1" Content="Питание"  HorizontalAlignment="Left" Margin="268,286,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="69" FontWeight="Bold" Style="{DynamicResource compoment_button}"/>
        <Button Background="LavenderBlush" Grid.Column="1" Content="Ось Y"  HorizontalAlignment="Left" Margin="20,345,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="69" FontWeight="Bold" Style="{DynamicResource compoment_button}"/>
        <Button Background="LavenderBlush" Grid.Column="1" Content="Ось В"  HorizontalAlignment="Left" Margin="103,345,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="69" FontWeight="Bold" Style="{DynamicResource compoment_button}"/>
        <Button Background="LavenderBlush" Grid.Column="1" Content="Магазин"  HorizontalAlignment="Left" Margin="185,345,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="69" FontWeight="Bold" Style="{DynamicResource compoment_button}"/>
        <Button Background="LavenderBlush" Grid.Column="1" Content="Шпиндель"  HorizontalAlignment="Left" Margin="268,345,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="69" FontWeight="Bold" Style="{DynamicResource compoment_button}"/>
        <Button Background="LavenderBlush" Grid.Column="1" Content="Ось Z" Margin="20,405,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" FontWeight="Bold" Style="{DynamicResource compoment_button}" HorizontalAlignment="Left" Width="69"/>
        <Button Background="LavenderBlush" Grid.Column="1" Content="Ось С"  HorizontalAlignment="Left" Margin="103,405,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="69" FontWeight="Bold" Style="{DynamicResource compoment_button}"/>
        <Button x:Name="SCHPU" Click="SCHPU_Click" Background="LavenderBlush" Grid.Column="1" Content="СЧПУ"  HorizontalAlignment="Left" Margin="185,405,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="69" FontWeight="Bold" Style="{DynamicResource compoment_button}"/>
        <Button Background="LavenderBlush" Grid.Column="1" Content="Панель"  HorizontalAlignment="Left" Margin="268,405,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="69" FontWeight="Bold" Style="{DynamicResource compoment_button}"/>
        <Button Background="LavenderBlush" Grid.Column="1" Content="Поворотный стол"  HorizontalAlignment="Left" Margin="352,286,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="121" FontWeight="Bold" Style="{DynamicResource compoment_button}"/>
        <Button Background="LavenderBlush" Grid.Column="1" Content="Технология"  HorizontalAlignment="Left" Margin="352,345,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="121" FontWeight="Bold" Style="{DynamicResource compoment_button}"/>
        <Button Background="LavenderBlush" Grid.Column="1" Content="Поворотная голова"  HorizontalAlignment="Left" Margin="352,405,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="121" FontWeight="Bold" Style="{DynamicResource compoment_button}"/>

        <!--график кнопка -->
        <Button x:Name="DrawGraph" Click="DrawGraph_click" Background="LavenderBlush" Grid.Column="1" Content="График"  HorizontalAlignment="Left" Margin="446,233,0,0" Grid.Row="1" VerticalAlignment="Top" Height="33" Width="74" Style="{DynamicResource compoment_button}" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="2">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-90"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>

        <!--Таблички -->
        <!-- СЧПУ -->
        <DataGrid x:Name="ActiveM" ItemsSource="{Binding}" 
          Grid.Column="1" Grid.Row="2" Grid.ColumnSpan="2" 
          Margin="20,20,20,20" AutoGenerateColumns="False"
          FontSize="14" FontWeight="Bold">

            <DataGrid.Resources>
                <!-- Общий стиль для всех ячеек -->
                <Style TargetType="DataGridCell">
                    <Setter Property="HorizontalContentAlignment" Value="Center"/>
                    <Setter Property="VerticalContentAlignment" Value="Center"/>
                    <Setter Property="Padding" Value="5"/>
                </Style>

                <!-- Стиль для текста внутри ячеек с триггерами -->
                <Style TargetType="TextBlock" x:Key="CenteredTextBlockStyle">
                    <Setter Property="TextAlignment" Value="Center"/>
                    <Setter Property="FontSize" Value="14"/>
                    <Setter Property="FontWeight" Value="DemiBold"/>

                </Style>
                <Style TargetType="TextBlock" x:Key="StatusStyle">
                    <Setter Property="TextAlignment" Value="Center"/>
                    <Setter Property="FontSize" Value="14"/>
                    <Setter Property="FontWeight" Value="DemiBold"/>
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding status}" Value="high">
                            <Setter Property="Foreground" Value="Red"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding status}" Value="medium">
                            <Setter Property="Foreground" Value="Orange"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding status}" Value="common">
                            <Setter Property="Foreground" Value="LightGreen"/>
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </DataGrid.Resources>

            <DataGrid.Columns>
                <DataGridTextColumn x:Name="ActiveMColumns" Header="Канал" Width="50" Binding="{Binding id}">
                    <DataGridTextColumn.ElementStyle>
                        <!--подключение стилей -->
                        <Style TargetType="TextBlock" BasedOn="{StaticResource CenteredTextBlockStyle}"/>
                    </DataGridTextColumn.ElementStyle>

                </DataGridTextColumn>

                <DataGridTextColumn Header="Время" Width="120" Binding="{Binding worktime}">
                    <DataGridTextColumn.ElementStyle>
                        <!--подключение стилей -->
                        <Style TargetType="TextBlock" BasedOn="{StaticResource CenteredTextBlockStyle}"/>
                    </DataGridTextColumn.ElementStyle>
                </DataGridTextColumn>

                <DataGridTextColumn Header="Статус" Width="120" Binding="{Binding status}">
                    <DataGridTextColumn.ElementStyle>
                        <!--подключение стилей -->
                        <Style TargetType="TextBlock" BasedOn="{StaticResource StatusStyle}"/>
                    </DataGridTextColumn.ElementStyle>
                </DataGridTextColumn>

                <DataGridTextColumn Header="Номер" Width="120" Binding="{Binding value}">
                    <DataGridTextColumn.ElementStyle>
                        <!--подключение стилей -->
                        <Style TargetType="TextBlock" BasedOn="{StaticResource CenteredTextBlockStyle}"/>
                    </DataGridTextColumn.ElementStyle>
                </DataGridTextColumn>

                <DataGridTextColumn Header="Текст" Width="550" Binding="{Binding description}">
                    <DataGridTextColumn.ElementStyle>
                        <!--подключение стилей -->
                        <Style TargetType="TextBlock" BasedOn="{StaticResource CenteredTextBlockStyle}"/>
                    </DataGridTextColumn.ElementStyle>
                </DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>


        <!-- Координаты-->
        <DataGrid x:Name ="Coords" Grid.Column="1" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Disabled" FontSize ="16" FontWeight="Bold" AutoGenerateColumns="False" ItemsSource="{Binding}" Margin="20,39,249,240" Grid.Row="1" Width="228">
            <DataGrid.Columns >
                <DataGridTextColumn x:Name="CoordsColumn" Header="      Оси" Width="100" Binding="{Binding Axe}"/>
                <DataGridTextColumn Header="  Координаты" Width="128" Binding="{Binding position}" />

            </DataGrid.Columns>
        </DataGrid>

        <!--Привода-->
        <DataGrid x:Name="Drives" AutoGenerateColumns="False" 
          ScrollViewer.HorizontalScrollBarVisibility="Disabled" 
          ScrollViewer.VerticalScrollBarVisibility="Disabled" 
          FontSize="16" FontWeight="Bold" 
          ItemsSource="{Binding}" 
          Grid.Column="1" Margin="260,39,30,240" Grid.Row="1">

            <DataGrid.Resources>
                <!-- Стиль для создания триггеров -->
                <Style TargetType="DataGridCell" x:Key="StatusCellKey" >
                    <Setter Property="HorizontalContentAlignment" Value="Center"/>
                    <Setter Property="VerticalContentAlignment" Value="Center"/>
                    <Setter Property="Padding" Value="5"/>
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding status}" Value="off">
                            <Setter Property="Background" Value="Red"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding status}" Value="on">
                            <Setter Property="Background" Value="Green"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding status}" Value="load">
                            <Setter Property="Background" Value="Orange"/>
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </DataGrid.Resources>

            <DataGrid.Columns>
                <DataGridTextColumn  Header="   Привод"  Width="100" Binding="{Binding driveLetter}"/>
                <DataGridTextColumn  Header="   Состояние"  Width="120" Binding="{Binding status}">
                    <DataGridTextColumn.CellStyle>
                        <!--подключение триггеров -->
                        <Style TargetType="DataGridCell" BasedOn="{StaticResource StatusCellKey}"/>
                    </DataGridTextColumn.CellStyle>
                </DataGridTextColumn>

            </DataGrid.Columns>
        </DataGrid>

        <Label Grid.Column="1" Content="Текущие значения координат" FontSize="16" FontWeight="DemiBold" HorizontalAlignment="Left" Margin="15,10,0,0" VerticalAlignment="Top" Height="34" Width="238" Grid.Row="1"/>
        <Label Grid.Column="1" Content="Состояние приводов" FontSize="16" FontWeight="DemiBold" HorizontalAlignment="Left" Margin="273,10,0,0" VerticalAlignment="Top" Height="34" Width="194" Grid.Row="1"/>
        <Label Grid.Column="1" Content="Активные сообщения" FontSize="16" FontWeight="DemiBold" HorizontalAlignment="Center" Margin="0,252,0,0" VerticalAlignment="Top" Height="34" Width="174" Grid.Row="1"/>



        <!--ГРАФИК -->
        <Label Grid.Column="2" Content="График прогрева шпинделя" FontSize="16" FontWeight="DemiBold" HorizontalAlignment="Left" Margin="130,10,0,0" Grid.Row="1" VerticalAlignment="Top" Width="289"/>

        <lvc:CartesianChart Background="White" Grid.Column="2" Series="{Binding SeriesCollection}" LegendLocation="Top" Margin="0,39,10,235" Grid.RowSpan="2" Grid.Row="1">
            <lvc:CartesianChart.AxisX>
                <lvc:Axis Title="Время" MinValue="0" MaxValue="480" Foreground="Black" FontSize="12"/>
            </lvc:CartesianChart.AxisX>
            <lvc:CartesianChart.AxisY>
                <lvc:Axis Title="Температура (°C)" Position="LeftBottom" MinValue="0" MaxValue="100" Foreground="Black" FontSize="12"/>
                <lvc:Axis Title="Обороты (RPM)" Position="RightTop" MinValue="0" MaxValue="8000" Foreground="Black" FontSize="12"/>
            </lvc:CartesianChart.AxisY>
        </lvc:CartesianChart>








        <!--Левая часть -->





        <Label Grid.Column="0" Content ="FakeAxiomaControl" FontSize="28" FontWeight="UltraBold" FontStyle="Italic" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="-1,0,0,0"/>
        <!--Левая часть. Четыре верхние кнопки -->
        <Button Style="{DynamicResource compoment_button}" FontSize="16" FontStyle="Italic" FontWeight="DemiBold" Background="white" HorizontalAlignment="Left" Margin="33,78,0,0" VerticalAlignment="Top" Height="46" Width="214" Grid.RowSpan="2">
            <StackPanel Width="203" Orientation="Horizontal">
                <Image Height="38" Width="43" Source="/home-01.png" Stretch="Fill"/>
                <TextBlock Text="                    Главная" Height="22" Width="157"/>


            </StackPanel>

        </Button>
        <Button Style="{DynamicResource compoment_button}" FontSize="16" FontStyle="Italic" FontWeight="DemiBold" Background="white" HorizontalAlignment="Left" Margin="33,60,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="213">
            <StackPanel Width="203" Orientation="Horizontal">
                <Image Height="38" Width="43" Stretch="Fill" Source="/alarm.png"/>
                <TextBlock Text="             Мониторинг" Height="22" Width="157"/>


            </StackPanel>

        </Button>
        <Button  Style="{DynamicResource compoment_button}" FontSize="16" FontStyle="Italic" FontWeight="DemiBold" Background="white" HorizontalAlignment="Left" Margin="33,126,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="213">
            <StackPanel Width="203" Orientation="Horizontal">
                <Image Height="38" Width="43" Stretch="Fill" Source="/graph-increase.png"/>
                <TextBlock Text="                       Анализ" Height="22" Width="157"/>


            </StackPanel>

        </Button>
        <Button   Style="{DynamicResource compoment_button}" FontSize="16" FontStyle="Italic" FontWeight="DemiBold" Background="white" HorizontalAlignment="Left" Margin="33,190,0,0" Grid.Row="1" VerticalAlignment="Top" Height="46" Width="212">

            <StackPanel Width="203" Orientation="Horizontal">
                <Image Height="38" Width="43" Stretch="Fill" Source="/bar-chart-01.png"/>
                <TextBlock Text="                      Отчет" Height="22" Width="157"/>

                <Border CornerRadius="6"/>
            </StackPanel>

        </Button>

        <!--Левая часть. Комбобоксы -->
        <Label Content="Тип Станка:" FontSize="16" FontWeight="Bold" HorizontalAlignment="Left" Margin="28,252,0,0" Grid.Row="1" VerticalAlignment="Top" RenderTransformOrigin="0.29,0.333" Height="31"/>
        <ComboBox x:Name ="mt_type" FontSize="16" FontWeight="DemiBold" DropDownClosed="type_mtDDClosed" HorizontalAlignment="Center" Margin="0,281,0,0" Grid.Row="1" VerticalAlignment="Top" Width="219" Height="31"/>
        <Label Content="Наименование:" FontSize="16" FontWeight="Bold" HorizontalAlignment="Left" Margin="28,304,0,0" Grid.Row="1" VerticalAlignment="Top" RenderTransformOrigin="0.29,0.333" Height="31"/>
        <ComboBox x:Name ="mt_name" FontSize="16" FontWeight="DemiBold" DropDownClosed="name_mtDDClosed" HorizontalAlignment="Center" Margin="0,335,0,0" Grid.Row="1" VerticalAlignment="Top" Width="219" Height="31"/>

        <!--Левая часть. рамка -->
        <Rectangle Fill="White" HorizontalAlignment="Left" Margin="36,371,0,10" Grid.Row="1" Stroke="Black" Width="210" Grid.RowSpan="2"/>

        <!--Левая часть. Картинка и кнопки под ней -->
        <Image x:Name="Image_machine" RenderTransformOrigin="0.5,0.5" Source="/IMU5xp.png" Stretch="Fill" Margin="38,373,40,151" Grid.RowSpan="2" Grid.Row="1"/>
        <Label x:Name="Nazvanie" Content="IMU-5XP 2518" HorizontalContentAlignment="Center" Background="White" FontSize ="20" FontWeight="Heavy" RenderTransformOrigin="0.5,0.5" Margin="40,68,41,123" Grid.Row="2"/>

        <Button Background="White" Margin="39,105,194,0" Grid.Row="2" VerticalAlignment="Top" Height="50" BorderThickness="1,1,1,1">
            <Image Height="41" Width="42" Source="/alarm.png"/>
        </Button>
        <Button Background="White"  HorizontalAlignment="Left" Margin="89,105,0,0" Grid.Row="2" VerticalAlignment="Top" Height="50" Width="52">
            <Image Height="41" Width="42" Source="/bar-chart-01.png"/>
        </Button>
        <Button Background="White" HorizontalAlignment="Left" Margin="141,105,0,0" Grid.Row="2" VerticalAlignment="Top" Height="50" Width="52">
            <Image Height="41" Width="42" Source="/alarm.png"/>
        </Button>
        <Button Background="White" HorizontalAlignment="Left" Margin="192,105,0,0" Grid.Row="2" VerticalAlignment="Top" Height="50" Width="52">
            <Image Height="41" Width="42" Source="/alarm.png"/>
        </Button>

        <Button Background="LawnGreen" FontWeight="ExtraBlack" Content="X" HorizontalAlignment="Left" Margin="39,160,0,0" Grid.Row="2" VerticalAlignment="Top" Height="55" Width="20" Style="{DynamicResource compoment_button}"/>
        <Button  Background="LawnGreen" FontWeight="ExtraBlack" Content="Y" HorizontalAlignment="Left" Margin="64,160,0,0" Grid.Row="2" VerticalAlignment="Top" Height="55" Width="20" Style="{DynamicResource compoment_button}"/>
        <Button Background="LawnGreen" FontWeight="ExtraBlack" Content="Z" HorizontalAlignment="Left" Margin="89,160,0,0" Grid.Row="2" VerticalAlignment="Top" Height="55" Width="20" Style="{DynamicResource compoment_button}"/>
        <Button Background="LawnGreen" FontWeight="ExtraBlack" Content="A" HorizontalAlignment="Left" Margin="114,160,0,0" Grid.Row="2" VerticalAlignment="Top" Height="55" Width="20" Style="{DynamicResource compoment_button}"/>
        <Button Background="LawnGreen" FontWeight="ExtraBlack" Content="B" HorizontalAlignment="Left" Margin="139,160,0,0" Grid.Row="2" VerticalAlignment="Top" Height="55" Width="20" Style="{DynamicResource compoment_button}"/>
        <Button Background="LawnGreen" FontWeight="ExtraBlack" Content="C" HorizontalAlignment="Left" Margin="164,160,0,0" Grid.Row="2" VerticalAlignment="Top" Height="55" Width="20" Style="{DynamicResource compoment_button}"/>
        <Button Background="LightBlue" FontWeight="ExtraBlack" Content="Auto" HorizontalAlignment="Left" Margin="187,160,0,0" Grid.Row="2" VerticalAlignment="Top" Height="55" Width="55" Style="{DynamicResource compoment_button}"/>
        

        <!--конец -->



    </Grid>
</Window>
